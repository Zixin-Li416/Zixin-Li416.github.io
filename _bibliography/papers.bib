@article{li2025reproducibility,
  title        = {Reproducibility in Business Statistics},
  author       = {Jessie Zixin Li and Ruizhu Zhou and Ivor Cribben},
  journal      = {The Review of Economics and Statistics},
  year         = {2025},
  note         = {Under Review},
  abbr         = {REStats},
  selected     = {true},
  pdf          = {Reproducibility_in_business_statistics.pdf},
  abstract     = {We examine the computational reproducibility of 235 articles published in seven prominent business statistics journals between January 2021 and July 2023. Despite clear journal policies requiring data and code sharing, only 39 articles (17%) could be fully or partially reproduced. We find that 87 (37%) articles provided real data and 121 (51%) provided code, but only 22 (9%) had fully working code. Common barriers include data unavailability, incomplete code, execution errors, and inadequate documentation. Our findings reveal substantial variation across journals, with enforcement of reproducibility policies mattering more than the policies themselves.}
}
